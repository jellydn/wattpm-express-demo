{
	"name": "express-v5",
	"version": "1.0.0",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"build": "tsup",
		"check": "biome check --apply .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"dev": "tsx watch app/index.ts",
		"start": "tsx app/index.ts",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@platformatic/node": "2.56.0",
		"body-parser": "1.20.3",
		"cors": "2.8.5",
		"express": "5.1.0",
		"helmet": "8.1.0",
		"jsend": "1.1.0",
		"pino": "9.6.0"
	},
	"devDependencies": {
		"@types/cors": "2.8.17",
		"@types/express": "5.0.1",
		"@types/jsend": "1.0.32",
		"@types/supertest": "6.0.3",
		"@vitest/coverage-v8": "2.1.9",
		"@vitest/ui": "2.1.9",
		"supertest": "7.1.0",
		"tsup": "8.4.0",
		"tsx": "4.19.3",
		"typescript": "5.8.2",
		"vitest": "2.1.9"
	},
	"tsup": {
		"entry": ["app/index.ts"],
		"splitting": false,
		"sourcemap": true,
		"clean": true
	}
}
